{
  "name": "@dojocoding/hacienda-cli",
  "version": "0.1.0",
  "description": "CLI for Costa Rica electronic invoicing (hacienda command)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "hacienda": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "costa-rica",
    "hacienda",
    "electronic-invoicing",
    "factura-electronica",
    "comprobantes-electronicos",
    "cli"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@dojocoding/hacienda-sdk": "workspace:*",
    "@dojocoding/hacienda-shared": "workspace:*",
    "citty": "^0.2.1"
  },
  "license": "MIT",
  "author": "Dojo Coding Labs <dev@dojocoding.io> (https://dojocoding.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/DojoCodingLabs/hacienda-cr.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/DojoCodingLabs/hacienda-cr#readme"
}
